<html>
    <body>
        <a href="/add">Add Products </a>
        <table border="2">
            <tr>
                <th>Name</th>
                <th>Image</th>
                <th>Price</th>
                <th>Description</th>
                <th>currency</th>
                
                <% data.forEach((item)=>{ %>
                    <tr>
                        <td><%=item.name%></td>
                        <td><img src="<%=item.img%>" width="100" height="100"/></td>
                        <td><%=item.Price%></td>
                        <td><%=item.description%></td>
                        <td><%=item.currency%></td>
                    </tr>
                <%})%>
            </tr>
        </table>
    </body>
</html>